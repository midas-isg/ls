<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://types.apollo.pitt.edu/v3_0_0/"
	xmlns:tns="http://types.apollo.pitt.edu/v3_0_0/" elementFormDefault="qualified">

	<simpleType name="Probability">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>


	<simpleType name="Fraction">
		<restriction base="double">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>

	<simpleType name="PositiveDouble">
		<restriction base="double">
			<minExclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="NonNegativeDouble">
		<restriction base="double">
			<minInclusive value="0" />
		</restriction>
	</simpleType>

	<simpleType name="SimulatorTypeEnum">
		<restriction base="string">
			<enumeration value="agent-based" />
			<enumeration value="compartmental" />
		</restriction>
	</simpleType>

	<simpleType name="UnitOfTimeEnum">
		<restriction base="string">
			<enumeration value="year" />
			<enumeration value="month" />
			<enumeration value="day" />
			<enumeration value="hour" />
			<enumeration value="minute" />
			<enumeration value="second" />
			<enumeration value="millisecond" />
		</restriction>
	</simpleType>

	
	<simpleType name="GenderEnum">
		<restriction base="string">
			<enumeration value="M" />
			<enumeration value="F" />
			<enumeration value="U" />
		</restriction>
	</simpleType>

	<simpleType name="SourceOfInfectionEnum">
		<restriction base="string">
			<enumeration value="syringe" />
			<enumeration value="infectiousHost" />
			<enumeration value="eitherSyringeOrInfectiousHost" />
		</restriction>
	</simpleType>

	







	<complexType name="SimulatorTimeSpecification">
		<sequence>
			<element name="unitOfTimeForSimulatorTimeStep" type="tns:UnitOfTimeEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="numberOfUnitsOfTimeInOneSimulatorTimeStep"
				maxOccurs="1" minOccurs="1" type="tns:PositiveDouble">
			</element>
			<element name="runLength" type="positiveInteger" maxOccurs="1"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="IndividualTreatmentControlStrategy">
		<complexContent>
			<extension base="tns:InfectiousDiseaseControlStrategy">
				<sequence>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="individualTreatment" type="tns:Treatment"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="populationTreatmentCensus" type="tns:PopulationTreatmentCensus"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="compliance" type="tns:ProbabilisticParameter"
						maxOccurs="1" minOccurs="1" />
					<element name="delayFromSymptomsToTreatment" type="tns:Duration"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="treatmentSystemLogistics" type="tns:TreatmentSystemLogistics"
						maxOccurs="unbounded" minOccurs="1">
					</element>
					<element name="targetPopulationsAndPrioritizations"
						type="tns:ControlStrategyTargetPopulationsAndPrioritization"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="Vaccination">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="vaccineId" type="tns:vaccineOntologyId"
						maxOccurs="1" minOccurs="1" />
					<element name="vaccinationEfficacies"
						type="tns:VaccinationEfficacyForSimulatorConfiguration" maxOccurs="unbounded"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RingIndividualTreatmentControlStrategy">
		<complexContent>
			<extension base="tns:IndividualTreatmentControlStrategy">
				<sequence>
					<element name="ringDiameterInMeters" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Infection">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="hostTaxonId" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="infectiousDiseases" type="tns:InfectiousDisease"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<choice>
						<element name="infectionAcquisitionsFromInfectiousHosts"
							type="tns:InfectionAcquisitionFromInfectiousHost" maxOccurs="unbounded"
							minOccurs="1">
						</element>
						<element name="infectionAcquisitionsFromContaminatedThings"
							type="tns:InfectionAcquisitionFromContaminatedThing" maxOccurs="unbounded"
							minOccurs="1">
						</element>
					</choice>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Contamination">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="pathogenTaxonId" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1" />
					<element name="abioticEcosystemElement" type="tns:AbioticEcosystemEnum"
						maxOccurs="1" minOccurs="1" />
					<element name="contaminationDuration" type="tns:Duration"
						maxOccurs="1" minOccurs="1" />
					<element name="contaminationAcquisition" type="tns:ContaminationAcquisition"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensus">
		<complexContent>
			<extension base="tns:Census">
				<sequence>
					<element name="populationSpecies" type="tns:NcbiTaxonId"
						maxOccurs="1" minOccurs="1" />
					<element name="pathogen" type="tns:ApolloPathogenCode"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="censusData"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="1"
						minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>
					<element name="censusDataCells"
						type="tns:PopulationInfectionAndImmunityCensusDataCell" maxOccurs="unbounded"
						minOccurs="1">
					</element>
					<element name="exceptionSubpopulations"
						type="tns:PopulationInfectionAndImmunityCensusData" maxOccurs="unbounded"
						minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PopulationTreatmentCensusData">
		<complexContent>
			<extension base="tns:CensusData">
				<sequence>

					<element name="treatmentCensusDataCells" type="tns:PopulationTreatmentCensusDataCell"
						maxOccurs="unbounded" minOccurs="1">
					</element>
					<element name="exceptionSubpopulations" type="tns:PopulationTreatmentCensusData"
						maxOccurs="unbounded" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	

	

	<simpleType name="ApolloLocationCode">
		<restriction base="token">
			<pattern value="[A-Z0-9]{2,17}"></pattern>
		</restriction>
	</simpleType>

	<complexType name="RequesterIdentification">
		<sequence>
			<element name="requesterName" type="token" />
			<element name="requesterCompany" type="token" />
			<element name="requesterEmail" type="token" />
		</sequence>
	</complexType>

	<complexType name="Vaccine">
		<sequence>
			<element name="description" type="string" maxOccurs="1"
				minOccurs="1" />
			<element name="vaccineIdentifier" type="token" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="valence" type="positiveInteger" maxOccurs="unbounded"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="AgeGroupEfficacy">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition" />
			<element name="vaccineEfficacy" type="tns:Fraction" />
		</sequence>
	</complexType>

	<complexType name="VaccineContraindications">
		<sequence>
			<element name="infants" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="preschoolers" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="schoolAgeChildren" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="youngAdults" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="olderAdults" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="elderly" type="boolean" maxOccurs="1"
				minOccurs="0" />
			<element name="pregnantAdultsRestricted" type="boolean"
				maxOccurs="1" minOccurs="0" />
		</sequence>
	</complexType>



	<complexType name="AntiviralTreatment">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="antiviralId" type="token" maxOccurs="1"
						minOccurs="1" />
					<element name="antiviralTreatmentEfficacy" type="tns:AntiviralTreatmentEfficacy"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="DrugTreatment">
		<complexContent>
			<extension base="tns:Treatment">
				<sequence>
					<element name="drugId" type="token" maxOccurs="1"
						minOccurs="1" />
					<element name="drugTreatmentEfficacy"
						type="tns:DrugTreatmentEfficacyForSimulatorConfiguration"
						maxOccurs="unbounded" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ArrayDimensionsDefinition" />

	<complexType name="TargetPopulationDefinition">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:GenderEnum" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="diseaseOutcome" type="tns:DiseaseOutcomeEnum"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="otherStratification" type="tns:PopulationStratificationEnum"
				maxOccurs="1" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<complexType name="PopulationInfectionAndImmunityCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:GenderEnum" maxOccurs="1"
				minOccurs="0">
			</element>
			<choice>
				<element name="infectionState" type="tns:InfectionStateEnum"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="diseaseState" type="tns:DiseaseOutcomeEnum"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
			<element name="fractionInState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />

		</sequence>
	</complexType>

	<complexType name="PopulationTreatmentCensusDataCell">
		<sequence>
			<element name="ageRange" type="tns:AgeRangeCategoryDefinition"
				maxOccurs="1" minOccurs="0">
			</element>
			<element name="gender" type="tns:GenderEnum" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="treatmentState" type="tns:TreatmentStateEnum"
				maxOccurs="1" minOccurs="1">
			</element>
			<element name="fractionInTreatmentState" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
		</sequence>

	</complexType>



	<complexType name="TemporalArrayDimensionsDefinition">
		<complexContent>
			<extension base="tns:ArrayDimensionsDefinition">
				<sequence>
					<element name="time" type="nonNegativeInteger" maxOccurs="unbounded"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="InfectionStateEnum">
		<restriction base="string">
			<enumeration value="susceptible" />
			<enumeration value="greaterThanOrEqualToId50Exposure" />
			<enumeration value="latent" />
			<enumeration value="infectious" />
			<enumeration value="recovered" />
			<enumeration value="immuneAndActsLikeImmune" />
			<enumeration value="immuneButActsLikeNotImmune" />
			<enumeration value="newly_sick" />
		</restriction>
	</simpleType>

	<simpleType name="TreatmentStateEnum">
		<restriction base="string">
			<enumeration value="treated" />
			<enumeration value="not_treated" />
		</restriction>
	</simpleType>

	<simpleType name="IndividualTreatmentEnum">
		<restriction base="string">
			<enumeration value="vaccination" />
			<enumeration value="antiviral_treatment" />
		</restriction>
	</simpleType>

	<complexType name="AgeRangeCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="unitOfTimeForLowerBound" type="tns:UnitOfTimeEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="lowerBound" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
					<element name="unitOfTimeForUpperBound" type="tns:UnitOfTimeEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="upperBound" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="DiseaseOutcomeCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="diseaseOutcome" type="tns:DiseaseOutcomeEnum"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TimeSpanCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="unitOfTime" type="tns:UnitOfTimeEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="timeZeroReference" type="tns:TimeScaleEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="startOfTimeSpan" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
					<element name="durationInTimeUnits" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="RealTimeSpanCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="offsetFromUtcInHours" type="integer"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="beginningTime" type="dateTime" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="endingTime" type="dateTime" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RealDateSpanCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="offsetFromUtcInHours" type="integer"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="firstDay" type="date" maxOccurs="1"
						minOccurs="1">
					</element>
					<element name="lastDay" type="date" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RealTimePointCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="offsetFromUtcInHours" type="integer"
						maxOccurs="1" minOccurs="0">
					</element>
					<element name="date" type="date" maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="TimeScaleEnum">
		<restriction base="string">
			<enumeration value="simulatorTimeScale" />
			<enumeration value="decsisionTimeScale" />
			<enumeration value="controlStrategyTimeScale" />
			<enumeration value="individualTreatmentTimeScale" />
			<enumeration value="epidemicTimeScale" />
			<enumeration value="gregorianCalendar" />
		</restriction>
	</simpleType>

	<complexType name="PlaceCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="place" type="tns:PlaceEnum" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GenderCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="gender" type="tns:GenderEnum" maxOccurs="1"
						minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SourceOfInfectionCategoryDefinition">
		<complexContent>
			<extension base="tns:CategoryDefinition">
				<sequence>
					<element name="sourceOfInfection" type="tns:SourceOfInfectionEnum"
						maxOccurs="1" minOccurs="1" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SimulatorTimeRange">
		<sequence>
			<element name="lowerBound" type="nonNegativeInteger" />
			<element name="upperBound" type="nonNegativeInteger" />
		</sequence>
	</complexType>

	<complexType name="TargetPriorityPopulation">
		<sequence>
			<element name="description" type="token" maxOccurs="1"
				minOccurs="1" />
			<choice>
				<element name="targetPopulationDefinition" type="tns:TargetPopulationDefinition"
					maxOccurs="1" minOccurs="1">
				</element>
				<element name="targetPopulationEnum" type="tns:TargetPopulationEnum"
					maxOccurs="1" minOccurs="1">
				</element>
			</choice>
			<element name="fractionOfTargetPopulationToPrioritize" type="tns:Fraction"
				maxOccurs="1" minOccurs="1" />
			<element name="priority" type="nonNegativeInteger" maxOccurs="1"
				minOccurs="1">
			</element>
		</sequence>
	</complexType>

	<complexType name="ControlStrategyTargetPopulationsAndPrioritization">
		<sequence>
			<choice>
				<element name="controlStrategyNamedPrioritizationScheme"
					type="tns:NamedPrioritizationSchemeEnum" maxOccurs="1" minOccurs="1">
				</element>
				<element name="controlStrategyTargetPopulationsAndPrioritization"
					type="tns:TargetPriorityPopulation" maxOccurs="unbounded"
					minOccurs="1">
				</element>
			</choice>
		</sequence>
	</complexType>

	<simpleType name="NamedPrioritizationSchemeEnum">
		<restriction base="string">
			<enumeration value="treat_sick_only"></enumeration>
			<enumeration value="ACIP"></enumeration>
			<enumeration value="HHTAP"></enumeration>
			<enumeration value="HHTAP100"></enumeration>
			<enumeration value="none"></enumeration>
		</restriction>
	</simpleType>


	<complexType name="InfectiousDiseaseControlStrategy">
		<complexContent>
			<extension base="tns:ApolloIndexableItem">
				<sequence>
					<element name="description" type="string" maxOccurs="1"
						minOccurs="1" />
					<element name="location" type="tns:Location" maxOccurs="1"
						minOccurs="0">
					</element>
					<element name="controlStrategyStartTime" type="tns:TriggerDefinition"
						maxOccurs="unbounded" minOccurs="1" />
					<element name="controlStrategyStopTime" type="tns:TriggerDefinition"
						maxOccurs="unbounded" minOccurs="0">
					</element>
					<element name="controlStrategyResponseDelay" type="tns:Duration"
						maxOccurs="1" minOccurs="1" />

					<element name="controlStrategyStandDownDelay" type="tns:Duration"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="referenceId" type="nonNegativeInteger"
						maxOccurs="1" minOccurs="0">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="DiseaseSurveillanceTriggerDefinition">
		<complexContent>
			<extension base="tns:TriggerDefinition">
				<sequence>
					<element name="reactiveControlStrategyTest" type="token"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlStrategyThreshold" type="int"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="unitOfMeasureForThreshold" type="tns:UnitOfMeasureEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlStrategyOperator" type="tns:OperatorEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="diseaseSurveillanceCapability" type="tns:DiseaseSurveillanceCapability"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="TemporalTriggerDefinition">
		<complexContent>
			<extension base="tns:TriggerDefinition">
				<sequence>
					<element name="timeScale" type="tns:TimeScaleEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="timeSinceTimeScaleZero" type="tns:FixedDuration"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="TreatmentSurveillanceTriggerDefinition">
		<complexContent>
			<extension base="tns:TriggerDefinition">
				<sequence>
					<element name="reactiveControlStrategyTest" type="token"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlStrategyThreshold" type="int"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="reactiveControlStrategyOperator" type="tns:OperatorEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="unitOfMeasureForThreshold" type="tns:UnitOfMeasureEnum"
						maxOccurs="1" minOccurs="1">
					</element>
					<element name="treatmentSurveillanceCapability" type="tns:TreatmentSurveillanceCapability"
						maxOccurs="1" minOccurs="1">
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>


	<complexType name="TriggerDefinition" />



	<simpleType name="TargetPopulationEnum">
		<restriction base="token">
			<enumeration value="all" />
			<enumeration value="members_of_the_same_household" />
			<enumeration value="newly_sick" />
			<enumeration value="household_member_of_infant" />
		</restriction>
	</simpleType>

	<simpleType name="PopulationStratificationEnum">
		<restriction base="token">
			<enumeration value="essential_workforce" />
			<enumeration value="pregnant" />
			<enumeration value="high_risk" />
		</restriction>
	</simpleType>



	

