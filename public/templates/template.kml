<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.opengis.net/kml/2.2">
<Document>
  <name>KML output from ALS</name>
  <description>KML output generated by Apollo Location Services (ALS)</description>
  <open>1</open>
  <Style id="epidemic-zone">
    <BalloonStyle>
      <text>
        <![CDATA[<font font size="3" color="#aa0000"><b>$[name]</b></font> <table border="1" padding="1">$[description]</table>]]>
      </text>
    </BalloonStyle>
    <LineStyle>
      <color>5f1400FF</color>
      <width>2</width>
    </LineStyle>
    <PolyStyle>
      <color>5f1400FF</color>
    </PolyStyle>      
  </Style>
  <Style id="admin-loc">
    <BalloonStyle>
      <text>
        <![CDATA[<font font size="3" color="#aa0000"><b>$[name]</b></font> <table border="1" padding="1">$[description]</table>]]>
      </text>
    </BalloonStyle>
    <LineStyle>
      <color>5a14B4F0</color>
      <width>0</width>
    </LineStyle>
    <PolyStyle>
      <color>5a14B4F0</color>
    </PolyStyle>
  </Style>
  <Placemark id="%d">
    <name><![CDATA[%s]]></name>
    <description><![CDATA[%s]]></description>
    <styleUrl>#admin-loc</styleUrl>
      %s
  </Placemark>
</Document>
</kml>